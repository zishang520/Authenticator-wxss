    position: absolute;
    top: 50%;
    transform: translate(0, -50%);
    width: 100%;
}*/

.full-screen {
  height:100vh;
}

.init .mdl-list {
    padding: 0;
    margin: 0;
}

.init .mdl-list .mdl-list__item {
    padding: 0;
    font-size: 14px;
}

.info .mdl-list {
    padding: 0;
    margin: 0;
}

.info .mdl-list .mdl-list__item {
    padding: 0;
    font-size: 14px;
}

.info .mdl-list__item-icon{
    margin-right: 16px !important;
}
.info .info-text{
    font-size: 28px;
}
.logo .img {
    width: 180px;
    height: 180px;
}



@supports (bottom: constant(safe-area-inset-bottom)) or (bottom: env(safe-area-inset-bottom)) {
    .block {
        margin-bottom: calc(120px + constant(safe-area-inset-bottom)) !important;
        margin-bottom: calc(120px + env(safe-area-inset-bottom)) !important;
    }
    .ipx {
        margin-bottom: constant(safe-area-inset-bottom);
        margin-bottom: env(safe-area-inset-bottom);
    }
}